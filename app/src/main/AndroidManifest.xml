<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pedro.personalprojects.ShoppingList">

    <application
        android:name=".app.ShoppingListApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ShoppingList">
        <activity android:name=".ui.registerList.RegisterListActivity"></activity>
        <activity android:name=".ui.listItem.ItemShoppingActivity"></activity>
        <activity android:name=".ui.registerItemList.RegisterItemShoppingActivity"></activity>

        <activity android:name=".ui.shoppingList.ShoppingListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>